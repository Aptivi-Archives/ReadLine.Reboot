<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyTitle>ReadLine.Reboot</AssemblyTitle>
    <AssemblyDescription>A reboot of tonerdo's ReadLine library for .NET</AssemblyDescription>
    <Description>A reboot of tonerdo's ReadLine library for .NET</Description>
    <Version>3.4.0</Version>
    <Authors>Toni Solarin-Sodara,Aptivi</Authors>
    <TargetFrameworks>net6.0;netcoreapp3.1;net48</TargetFrameworks>
    <DebugType>portable</DebugType>
    <AssemblyName>ReadLine.Reboot</AssemblyName>
    <PackageId>ReadLine.Reboot</PackageId>
    <PackageTags>readline;gnu;console;shell;cui</PackageTags>
    <PackageProjectUrl>https://github.com/Aptivi/ReadLine.Reboot</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Aptivi/ReadLine.Reboot</RepositoryUrl>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Company>Aptivi</Company>
    <Copyright>Copyright (c) 2022 Aptivi.</Copyright>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <ApplicationIcon>OfficialAppIcon-ReadLineReboot.ico</ApplicationIcon>
    <PackageIcon>OfficialAppIcon-ReadLineReboot-512.png</PackageIcon>
    <AssemblyVersion>3.4.0</AssemblyVersion>
    <PackageVersion>3.4.0</PackageVersion>
    <LangVersion>9.0</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="OfficialAppIcon-ReadLineReboot.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Include="OfficialAppIcon-ReadLineReboot-512.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
